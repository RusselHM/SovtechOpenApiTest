<script type="text/javascript">
    $(document).ready(function () {
        $(function () {
            $('#btnDetails').click(function (event) {
                event.preventDefault();

                $.ajax({
                    type: "GET",
                    url: "/Swapi/GetSwapi/",
                    dataType: "json",
                    data: { searchTerm: "animal" },
                    contentType: 'application/json; charset=utf-8',
                    success: function (data) {
                        alert(data.msg);
                    },
                    error: function () {
                        alert("Error occured!!")
                    }
                });
            });
        });
    });
</script>